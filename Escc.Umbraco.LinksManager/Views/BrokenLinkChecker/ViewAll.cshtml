@model Escc.Umbraco.LinksManager.Models.BrokenLinks.BrokenLinksViewModel

<p></p>
<div class="panel panel-info">
    <div class="panel-heading">
        <h3 class="panel-title">Broken Links [@Model.ViewResultsFrom - @(Model.ViewResultsFrom + 999)]</h3>
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-sm-9 col-sm-offset-1 col-md-9 col-md-offset-1 col-lg-9 col-lg-offset-1">
                @Html.RouteLink("<< 1000", "Previous", new { ViewResultsFrom = Model.ViewResultsFrom }, new { @class = "btn btn-info" })            </div>
            <div class="col-sm-2 col-md-2 col-lg-2">
                @Html.RouteLink("1000 >>", "Next", new { ViewResultsFrom = Model.ViewResultsFrom }, new { @class = "btn btn-info" })
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12">
                <p></p>
                @{Html.RenderPartial("~/Views/Partials/_DataTable.cshtml", Model.BrokenLinksTable);}
            </div>
        </div>
        <div class="row">
            <div class="col-sm-9 col-sm-offset-1 col-md-9 col-md-offset-1 col-lg-9 col-lg-offset-1">
                @Html.RouteLink("<< 1000", "Previous", new { ViewResultsFrom = Model.ViewResultsFrom }, new { @class = "btn btn-info" })            </div>
            <div class="col-sm-2 col-md-2 col-lg-2">
                @Html.RouteLink("1000 >>", "Next", new { ViewResultsFrom = Model.ViewResultsFrom}, new { @class = "btn btn-info" })  
            </div>
        </div>
    </div>
